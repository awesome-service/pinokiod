<style>
html {
  scroll-behavior: smooth;
  height: 100%;
  overflow: hidden;
}

body {
  height: 100%;
  overflow: hidden;
  display: flex;
  flex-direction: column;
}
#table > table {
  border-left: 2px solid black;
  padding-left: 5px;
  margin: 10px 0;
}
.highlight {
  background: yellow;
  width: 100px;
}
th {
  background: rgba(0,0,0,0.1);
  padding: 10px;
  font-size: 12px;
